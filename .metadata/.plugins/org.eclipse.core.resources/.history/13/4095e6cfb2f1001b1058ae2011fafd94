	int numeroUno, numeroDos;

	printf("Ingrese el primer operando: ");
	scanf("%d", &numeroUno);
	printf("Ingrese el primer operando: ");
	scanf("%d", &numeroDos);
	return EXIT_SUCCESS;
}
