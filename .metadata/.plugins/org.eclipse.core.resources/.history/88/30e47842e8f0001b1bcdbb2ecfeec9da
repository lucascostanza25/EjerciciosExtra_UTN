#include <stdio.h>
#include <stdlib.h>

int main(void) {
	setbuf(stdout, NULL);
	int i;
	int numeroIngresado;

	for(i=0; i<5; i++)
	{
		printf("Ingrese 5 numeros (%d/5): ", i);
		scanf("%d", &numeroIngresado);
	}

	return EXIT_SUCCESS;
}
